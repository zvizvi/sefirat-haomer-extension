    "storage"
  ],
  "options_ui": {
    "page": "html/options.html"
  },
  "icons": {
    "128": "img/icon128.png",
    "96": "img/icon96.png",
    "72": "img/icon72.png",
    "64": "img/icon64.png",
    "48": "img/icon48.png",
    "32": "img/icon32.png",
    "24": "img/icon24.png",
    "16": "img/icon16.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
